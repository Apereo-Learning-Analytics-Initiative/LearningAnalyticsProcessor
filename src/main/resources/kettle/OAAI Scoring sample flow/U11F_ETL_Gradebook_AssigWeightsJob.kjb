    <entry>
      <name>Sum_Max_Points</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;-- calculating the sum of max points for all the grading rubric courses and alternative &#xd;&#xa;-- combinations&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;create table Temp01 select course, alternative_ID, sum&#x28;max_points&#x29; as sum_max_points&#xa;from Grades_NotNULL&#xa;where weight in &#x28;-999,0&#x29;&#xa;group by course, alternative_ID&#xa;order by course,alternative_ID&#x3b;</sql>
