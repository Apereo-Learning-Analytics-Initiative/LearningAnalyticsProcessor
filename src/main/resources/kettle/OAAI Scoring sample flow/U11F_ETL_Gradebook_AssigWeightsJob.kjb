    <entry>
      <name>calculate weights</name>
      <description/>
      <type>SQL</type>
      <sql>---------------------------------------------------------------------------------------&#xd;&#xa;-- Assigning calculated weights to courses having -999 as weights using the grading rubric selected in previous step&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;Create table Temp07 select  G.course, G.alternative_ID,  G.max_points&#x2f;T6.sum_max_points AS  Calc_Weight, G.category,G.Gradable_object, G.max_points, G.earned_points&#xa;from Grades_NotNULL G, Temp06 T6&#xa;where G.Course &#x3d; T6.course&#xa;and weight in &#x28;-999,0&#x29;&#xa;group by g.course, g.alternative_id,g.Max_Points,g.category,G.Gradable_object,G.earned_points, T6.sum_max_points&#x3b;&#xa;&#xa;</sql>
