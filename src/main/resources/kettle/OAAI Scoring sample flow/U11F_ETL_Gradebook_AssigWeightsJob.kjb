    <entry>
      <name>Dummy_NullWeights</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;-- Selecting records from courses where Weights are not provided by the instructor in the &#xd;&#xa;-- grading rubric&#xd;&#xa;--&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;update  Grades_DistAll &#xa;set     Weight &#x3d; -999 &#xa;where   ALTERNATIVE_ID IN &#x28; SELECT CWID FROM &#x28;select distinct ALTERNATIVE_ID AS CWID&#xa;						 from Grades_DistAll &#xa;						 where weight &#x3d; &#x27;&#x5c;&#x5c;n&#x27;&#x29; AS c&#x29;&#xa;    and Course IN &#x28;SELECT CID FROM &#x28;select distinct Course AS CID&#xa;						 from Grades_DistAll &#xa;						 where weight &#x3d; &#x27;&#x5c;&#x5c;n&#x27;&#x29; AS b&#x29;&#xa;	and Weight &#x3d; &#x27;&#x5c;&#x5c;n&#x27;&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
