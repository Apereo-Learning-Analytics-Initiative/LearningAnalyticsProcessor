      <sql>---------------------------------------------------------------------------------------&#xd;&#xa;-- Setting the maximum percentage frequency as the standard grading rubric for the course&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;create table Temp06 select T4.course, T4.sum_max_points, T5.max_pctfreq&#xa;from Temp04 T4, Temp05 T5&#xa;where T4.course &#x3d; T5.course&#xa;and T4.pctfreq &#x3d; T5.max_pctfreq&#x3b;</sql>
