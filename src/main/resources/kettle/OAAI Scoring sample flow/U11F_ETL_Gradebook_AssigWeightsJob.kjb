    <entry>
      <name>Frequency_Percentage</name>
      <description/>
      <type>SQL</type>
      <sql>---------------------------------------------------------------------------------------&#xd;&#xa;-- calculating percentage frequency by diving against frequency by total frequency&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;create table temp04 select T2.course, T2.sum_max_points, T2.freq, &#x28;T2.freq &#x2a; 1.00 &#x2f; T3.totfreq&#x29;&#x2a;100 as pctfreq&#xa;from Temp02 T2 inner join Temp03 T3&#xa;on T2.course &#x3d; T3.course&#x3b;</sql>
