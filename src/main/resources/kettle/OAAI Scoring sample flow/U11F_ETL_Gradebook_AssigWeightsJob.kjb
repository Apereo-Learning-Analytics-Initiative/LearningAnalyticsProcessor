    <entry>
      <name>Update Gradebook</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;-- Merging calculated weights with the atual Gradebook table to improve more records to have &#xd;&#xa;-- more gradable objects having weights associated&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;ALTER TABLE grades_cw&#xa;ADD INDEX &#x60;agga&#x60; &#x28;&#x60;COURSE&#x60; ASC, &#x60;ALTERNATIVE_ID&#x60; ASC, &#x60;Weight&#x60; ASC&#x29;&#x3b;&#xa;&#xa;&#xa;UPDATE  Grades_CW CW&#xa;        INNER JOIN Grades_CalculatedWeights G&#xa;            ON CW.course &#x3d; G.Course&#xa; and CW.alternative_ID &#x3d; G.Alternative_ID &#xa;   and CW.Gradable_object &#x3d; G.Gradable_object&#xa;   and CW.Weight in &#x28;-999,0&#x29;&#xa;SET     CW.Weight &#x3d; G.Calc_Weight&#x3b;&#xa;&#xa;&#xa;drop table if exists GRADES&#x3b;&#xa;&#xa;create table GRADES select &#x2a; from grades_cw&#x3b;&#xa;&#xa;&#xa;update GRADES&#xa;set weight &#x3d; 0&#xa;where Weight &#x3d; -999&#x3b;&#xa;&#xa;&#xa;drop table Grades_CW&#x3b;&#xa;drop table Grades_CalculatedWeights&#x3b;&#xa;drop table Grades_NotNULL&#x3b;&#xa;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>OAAIDBDemo_connect</connection>
