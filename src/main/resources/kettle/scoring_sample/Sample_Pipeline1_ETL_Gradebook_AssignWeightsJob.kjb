    <entry>
      <name>Update Gradebook</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;-- Merging calculated weights with the atual Gradebook table to improve more records to have &#xd;&#xa;-- more gradable objects having weights associated&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;ALTER TABLE GRADES_CW&#xd;&#xa;ADD INDEX &#x60;agga&#x60; &#x28;&#x60;COURSE_ID&#x60; ASC, &#x60;ALTERNATIVE_ID&#x60; ASC, &#x60;WEIGHT&#x60; ASC&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE  GRADES_CW CW&#xd;&#xa;        INNER JOIN GRADES_CALCULATEDWEIGHTS G&#xd;&#xa;            ON CW.COURSE_ID &#x3d; G.COURSE_ID&#xd;&#xa;	AND CW.ALTERNATIVE_ID &#x3d; G.ALTERNATIVE_ID &#xd;&#xa;	AND CW.GRADABLE_OBJECT &#x3d; G.GRADABLE_OBJECT&#xd;&#xa;	AND CW.WEIGHT in &#x28;-999,0&#x29;&#xd;&#xa;SET     CW.WEIGHT &#x3d; G.CALC_WEIGHT&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS GRADES&#x3b;&#xd;&#xa;&#xd;&#xa;CREATE TABLE GRADES SELECT &#x2a; FROM GRADES_CW&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;UPDATE GRADES&#xd;&#xa;SET WEIGHT &#x3d; 0&#xd;&#xa;WHERE WEIGHT &#x3d; -999&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE GRADES_CW&#x3b;&#xd;&#xa;DROP TABLE GRADES_CALCULATEDWEIGHTS&#x3b;&#xd;&#xa;DROP TABLE GRADES_NOTNULL&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>185</xloc>
      <yloc>209</yloc>
      </entry>
    <entry>
      <name>Delete Hybrid courses</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;-- The point is to enrich the gradebook metric for those course which do not use&#xd;&#xa;-- weights for the grading rubric. Eliminating hybrid courses as they are outliers&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DELETE &#xd;&#xa;FROM  DISTINCT_GRADES&#xd;&#xa;WHERE COURSE_ID IN &#x28;SELECT CWID FROM &#x28;SELECT DISTINCT COURSE_ID AS CWID&#xd;&#xa;				  FROM   DISTINCT_GRADES&#xd;&#xa;				  WHERE  WEIGHT NOT IN&#x28;-999&#x29;&#x29; AS c&#x29; &#xd;&#xa;					 AND COURSE_ID IN &#x28;SELECT CID FROM &#x28;SELECT COURSE_ID AS CID&#xd;&#xa;									FROM   DISTINCT_GRADES&#xd;&#xa;									WHERE  WEIGHT &#x3d; -999&#x29; AS d&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;CREATE TABLE GRADES_NOTNULL SELECT &#x2a; FROM DISTINCT_GRADES&#x3b;&#xd;&#xa;DROP TABLE DISTINCT_GRADES&#x3b;	</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>574</xloc>
      <yloc>79</yloc>
      </entry>
