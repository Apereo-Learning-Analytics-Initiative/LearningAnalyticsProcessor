      <sql>CREATE TABLE GRADES_CW  SELECT &#x2a; &#xd;&#xa;FROM GRADES_NOTNULL&#x3b;&#xd;&#xa;&#xd;&#xa;CREATE TABLE GRADES_CALCULATEDWEIGHTS SELECT &#x2a; FROM RUBRIC_TEMP_07&#x3b;</sql>
