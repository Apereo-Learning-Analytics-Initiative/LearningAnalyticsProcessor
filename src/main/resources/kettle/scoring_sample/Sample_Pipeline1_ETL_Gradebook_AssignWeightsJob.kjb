    <entry>
      <name>Dummy_NullWeights</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;-- Selecting records from courses where Weights are not provided by the instructor in the &#xd;&#xa;-- grading rubric&#xd;&#xa;--&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS DISTINCT_GRADES&#x3b;&#xd;&#xa;CREATE TABLE DISTINCT_GRADES SELECT DISTINCT &#x2a; FROM &#x24;&#x7b;GRADE&#x7d;&#x3b;&#xd;&#xa;ALTER TABLE DISTINCT_GRADES ADD PRIMARY KEY &#x28;ALTERNATIVE_ID, COURSE_ID, GRADABLE_OBJECT&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE  DISTINCT_GRADES &#xd;&#xa;SET WEIGHT &#x3d; -999 &#xd;&#xa;WHERE ALTERNATIVE_ID IN &#x28; SELECT CWID FROM &#x28;SELECT DISTINCT ALTERNATIVE_ID AS CWID&#xd;&#xa;						 FROM DISTINCT_GRADES &#xd;&#xa;						 WHERE WEIGHT IS NULL&#x29; AS c&#x29;&#xd;&#xa;    AND COURSE_ID IN &#x28;SELECT CID FROM &#x28;SELECT DISTINCT COURSE_ID AS CID&#xd;&#xa;						 FROM DISTINCT_GRADES &#xd;&#xa;						 WHERE WEIGHT IS NULL&#x29; AS b&#x29;&#xd;&#xa;	AND WEIGHT IS NULL&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>220</xloc>
      <yloc>79</yloc>
      </entry>
    <entry>
      <name>Zero_EarnedPoint_Nulls</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;-- If earned point is null then associated a zero as the entry&#xd;&#xa;-- &#xd;&#xa;--&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;UPDATE DISTINCT_GRADES &#xd;&#xa;SET EARNED_POINTS &#x3d; 0 &#xd;&#xa;WHERE EARNED_POINTS IS NULL&#x3b;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>385</xloc>
      <yloc>81</yloc>
      </entry>
    <entry>
      <name>Sum_Max_Points</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;-- calculating the sum of max points for all the grading rubric courses and alternative &#xd;&#xa;-- combinations&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;CREATE TABLE RUBRIC_TEMP_01 SELECT COURSE_ID, ALTERNATIVE_ID, SUM&#x28;MAX_POINTS&#x29; AS SUM_MAX_POINTS&#xd;&#xa;FROM GRADES_NOTNULL&#xd;&#xa;WHERE WEIGHT IN &#x28;-999,0&#x29;&#xd;&#xa;GROUP BY COURSE_ID, ALTERNATIVE_ID&#xd;&#xa;ORDER BY COURSE_ID,ALTERNATIVE_ID&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>764</xloc>
      <yloc>79</yloc>
      </entry>
    <entry>
      <name>Compute_frequency</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;-- computing frequency for of occurence for the sum of points for the course. &#xd;&#xa;-- This gets the majority grading rubric for the course&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;CREATE TABLE RUBRIC_TEMP_02 SELECT COURSE_ID, SUM_MAX_POINTS, COUNT&#x28;&#x2a;&#x29;  as FREQ&#xd;&#xa;FROM RUBRIC_TEMP_01&#xd;&#xa;GROUP BY COURSE_ID, SUM_MAX_POINTS&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>939</xloc>
      <yloc>79</yloc>
      </entry>
    <entry>
      <name>Total_Frequency</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;-- calculating sum for all the frequency&#xd;&#xa;---------------------------------------------------------------------------------------v&#xd;&#xa;&#xd;&#xa;CREATE TABLE RUBRIC_TEMP_03 SELECT COURSE_ID, SUM&#x28;FREQ&#x29; AS TOTFREQ&#xd;&#xa;FROM RUBRIC_TEMP_02&#xd;&#xa;GROUP BY COURSE_ID&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>1118</xloc>
      <yloc>83</yloc>
      </entry>
    <entry>
      <name>Frequency_Percentage</name>
      <description/>
      <type>SQL</type>
      <sql>---------------------------------------------------------------------------------------&#xd;&#xa;-- calculating percentage frequency by diving against frequency by total frequency&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;CREATE TABLE  RUBRIC_TEMP_04 SELECT T2.COURSE_ID, T2.SUM_MAX_POINTS, T2.FREQ, &#x28;T2.FREQ &#x2a; 1.00 &#x2f; T3.TOTFREQ&#x29;&#x2a;100 as PCTFREQ&#xd;&#xa;FROM RUBRIC_TEMP_02 T2 INNER JOIN RUBRIC_TEMP_03 T3&#xd;&#xa;ON T2.COURSE_ID &#x3d; T3.COURSE_ID&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>1179</xloc>
      <yloc>203</yloc>
      </entry>
    <entry>
      <name>Max_Percentage</name>
      <description/>
      <type>SQL</type>
      <sql>---------------------------------------------------------------------------------------&#xd;&#xa;-- Putting a percentage cut off so filter out low frequency grading rubrics for the course&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;CREATE TABLE RUBRIC_TEMP_05 SELECT COURSE_ID, MAX&#x28;PCtFREQ&#x29; AS MAX_PCTFREQ &#xd;&#xa;FROM RUBRIC_TEMP_04&#xd;&#xa;GROUP BY COURSE_ID&#xd;&#xa;HAVING MAX&#x28;PCTFREQ&#x29;  &#x3e;&#x3d; 75&#x3b; </sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>1021</xloc>
      <yloc>202</yloc>
      </entry>
    <entry>
      <name>Sum_max_points_MaxPercentage</name>
      <description/>
      <type>SQL</type>
      <sql>---------------------------------------------------------------------------------------&#xd;&#xa;-- Setting the maximum percentage frequency as the standard grading rubric for the course&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;CREATE TABLE RUBRIC_TEMP_06 SELECT T4.COURSE_ID, T4.SUM_MAX_POINTS, T5.MAX_PCTFREQ&#xd;&#xa;FROM RUBRIC_TEMP_04 T4, RUBRIC_TEMP_05 T5&#xd;&#xa;WHERE T4.COURSE_ID &#x3d; T5.COURSE_ID&#xd;&#xa;AND T4.PCTFREQ &#x3d; T5.MAX_PCTFREQ&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>858</xloc>
      <yloc>203</yloc>
      </entry>
    <entry>
      <name>calculate weights</name>
      <description/>
      <type>SQL</type>
      <sql>---------------------------------------------------------------------------------------&#xd;&#xa;-- Assigning calculated weights to courses having -999 as weights using the grading rubric selected in previous step&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;CREATE TABLE RUBRIC_TEMP_07 SELECT  G.COURSE_ID, G.ALTERNATIVE_ID,  G.MAX_POINTS&#x2f;T6.SUM_MAX_POINTS AS  CALC_WEIGHT, G.CATEGORY,G.GRADABLE_OBJECT, G.MAX_POINTS, G.EARNED_POINTS&#xd;&#xa;FROM GRADES_NOTNULL G, RUBRIC_TEMP_06 T6&#xd;&#xa;WHERE G.COURSE_ID &#x3d; T6.COURSE_ID&#xd;&#xa;AND WEIGHT IN &#x28;-999,0&#x29;&#xd;&#xa;GROUP BY G.COURSE_ID, G.ALTERNATIVE_ID, G.MAX_POINTS, G.CATEGORY, G.GRADABLE_OBJECT, G.EARNED_POINTS, T6.SUM_MAX_POINTS&#x3b;&#xa;&#xa;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>LAP_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>677</xloc>
      <yloc>203</yloc>
      </entry>
