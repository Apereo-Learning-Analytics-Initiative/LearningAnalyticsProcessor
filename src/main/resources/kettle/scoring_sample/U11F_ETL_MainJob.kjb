    <entry>
      <name>GradBio_PK</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;--------------------------------------------------------------------------------------&#xd;&#xa;-- Kettle doesn&#x27;t seem to accept multiline comments. Hence using single line comments&#xd;&#xa;-- Not required if extracts are coming from H2 base extracts&#xd;&#xa;--&#xd;&#xa;---------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;-- alter table u11fbio_test modify alternative_id varchar&#x28;36&#x29; not null&#x3b;&#xa;&#xd;&#xa;--------------------------------------------------------------------------------&#xd;&#xa;-- CREATE A temporary table DISTBIO from the base BIO file.   &#xd;&#xa;-- Check and eliminate duplicate entries if any by using Row number option.&#xd;&#xa;--------------------------------------------------------------------------------&#xd;&#xa;&#xa;drop table if exists DISTBIO&#x3b;&#xa;&#xa;create table DISTBIO select &#x2a; from &#x28;SELECT  &#x40;row_num &#x3a;&#x3d; IF&#x28;&#x40;prev_value&#x3d;o.alternative_id,&#x40;row_num&#x2b;1,1&#x29; AS RowNumber&#xa;       ,&#x40;prev_value &#x3a;&#x3d; o.alternative_id as alternative_id, &#xa;CLASS_RANK, &#xa;PERCENTILE, &#xa;SAT_VERBAL_SCORE, &#xa;SAT_MATH_SCORE, &#xa;ACT_COMPOSITE_SCORE, &#xa;AGE, &#xa;GENDER, &#xa;FTPT, &#xa;CLASS, &#xa;CUM_GPA, &#xa;SEM_GPA, &#xa;ACADEMIC_STANDING&#xa;  FROM &#x24;&#x7b;BIO&#x7d;  o,&#xa;      &#x28;SELECT &#x40;row_num &#x3a;&#x3d; 1&#x29; x,&#xa;      &#x28;SELECT &#x40;prev_value &#x3a;&#x3d; &#x27;&#x27;&#x29; y&#xa;  ORDER BY o.ALTERNATIVE_ID DESC&#x29; as c&#xa;where RowNumber &#x3d; 1&#xa;order by alternative_id&#x3b;&#xa;alter table DISTBIO drop column RowNumber&#x3b;&#xa;ALTER ignore TABLE DISTBIO &#xa; ADD  CONSTRAINT PK_DISTBIO &#xa;PRIMARY KEY &#x28;ALTERNATIVE_ID&#x29;&#x3b;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>OAAIDBDemo_connect</connection>
