    <entry>
      <name>Pk_GradCourse</name>
      <description/>
      <type>SQL</type>
      <sql>----------------------------------------------------------------------&#xd;&#xa;-- Eliminates duplicate entries for Course. This was necessary as there were &#xd;&#xa;-- repetitions in the csv files received. We use row numbers to identify&#xd;&#xa;-- duplicates and write distinct entries to a different table. &#xd;&#xa;-----------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;drop table if exists DISTCOURSE &#x3b;&#xa;create table DISTCOURSE SELECT &#x2a; FROM &#x28;&#xa; SELECT &#xa;   &#x40;row_num &#x3a;&#x3d; IF&#x28;&#x40;prev_value&#x3d;CONCAT_WS&#x28;&#x27;&#x27;, ALTERNATIVE_ID, COURSE&#x29;, &#x40;row_num &#x2b; 1, 1&#x29; AS RowNumber&#xa;        ,ALTERNATIVE_ID&#xa;        ,m.COURSE&#xa;        ,m.SUBJECT&#xa;        ,m.COURSENO&#xa;         ,m.SECTION&#xa;		 ,m.ENROLLMENT&#xa;         ,m.LETTER_GRADE&#xa;         ,m.COURSENUM&#xa;         ,m.ONLINE_FLAG&#xa;         ,&#x40;prev_value &#x3a;&#x3d; CONCAT_WS&#x28;&#x27;&#x27;,ALTERNATIVE_ID, COURSE&#x29;&#xa;  FROM &#x24;&#x7b;COURSE&#x7d; m,&#xa;      &#x28;SELECT &#x40;row_num &#x3a;&#x3d; 1&#x29; x,&#xa;      &#x28;SELECT &#x40;prev_value &#x3a;&#x3d; &#x27;&#x27;&#x29; y&#xa;   ORDER BY m.ALTERNATIVE_ID, m.COURSE DESC&#xa;&#x29; AS A&#xa;WHERE A.RowNumber &#x3d;1&#x3b;&#xa;alter table DISTCOURSE drop RowNumber &#x3b;&#xa;ALTER TABLE DISTCOURSE modify alternative_id varchar&#x28;36&#x29; not null&#x3b;&#xa;ALTER TABLE DISTCOURSE modify course varchar&#x28;17&#x29; not null&#x3b;&#xa;ALTER ignore TABLE DISTCOURSE&#xa; ADD  CONSTRAINT PK_DISTCOURSE&#xa;PRIMARY KEY &#x28;ALTERNATIVE_ID,course&#x29;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>OAAIDBDemo_connect</connection>
