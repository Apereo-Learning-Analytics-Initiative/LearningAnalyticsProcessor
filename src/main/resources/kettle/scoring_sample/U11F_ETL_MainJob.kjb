    <entry>
      <name>PC</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;----------------------------------------------------------------------------------&#xd;&#xa;-- First stage of merging. Includes merging of Demographics &#x28;BIO&#x29; table and   &#xd;&#xa;-- Course Enrollment table. For our analysis we are deleting the letter grades&#xd;&#xa;-- which are not valid. Technically for scoring purposes we wont have the &#xd;&#xa;-- grade information since we will be predicting it. So we can eliminate &#xd;&#xa;-- the delete query for grade info&#xd;&#xa;---------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;drop table if exists PC&#x3b;&#xa;&#xd;&#xa;&#xd;&#xa;create table PC&#xa;SELECT  P.ALTERNATIVE_ID,&#xa;C.COURSE,&#xa;C.COURSENUM,&#xa;C.SUBJECT,&#xa;C.ONLINE_FLAG,&#xa;C.SECTION,&#xa;C.ENROLLMENT,&#xa;C.LETTER_GRADE,&#xa;P.CLASS_RANK,&#xa;P.PERCENTILE,&#xa;P.SAT_VERBAL_SCORE,&#xa;P.SAT_MATH_SCORE,&#xa;P.ACT_COMPOSITE_SCORE,&#xa;P.APTITUDE_SCORE,&#xa;P.AGE,&#xa;P.GENDER,&#xa;P.FTPT,&#xa;P.CLASS,&#xa;P.CUM_GPA,&#xa;P.SEM_GPA,&#xa;P.ACADEMIC_STANDING&#xa;FROM DISTBIO  P&#xa;INNER JOIN DISTCOURSE  C&#xa;&#xa;ON P.ALTERNATIVE_ID &#x3d;C.ALTERNATIVE_ID&#x3b;&#xa;&#xd;&#xa;&#xd;&#xa;&#xa;-- delete from PC where Letter_Grade in &#x28;&#x27;NC&#x27;,&#x27;U&#x27;,&#x27;AU&#x27;,&#x27;WF&#x27;,&#x27;W&#x27;,&#x27;P&#x27;,&#x27;I&#x27;&#x29;&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>OAAIDBDemo_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>462</xloc>
      <yloc>369</yloc>
      </entry>
    <entry>
      <name>Recodin</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;----------------------------------------------------------------------&#xd;&#xa;-- Recoding Gender information into numeric for data mining purposes&#xd;&#xa;--&#xd;&#xa;--&#xd;&#xa;----------------------------------------------------------------------&#xd;&#xa;&#xa;Update PC &#xa;       Set GENDER &#x3d; CASE&#xd;&#xa;			WHEN GENDER &#x3d; &#x27;F&#x27; THEN &#x27;1&#x27;&#xd;&#xa;			WHEN GENDER &#x3d; &#x27;M&#x27; THEN &#x27;2&#x27;&#xd;&#xa;			ELSE NULL&#xd;&#xa;		END&#x3b;&#xd;&#xa;			&#xd;&#xa;----------------------------------------------------------------------&#xd;&#xa;-- Recoding Gender information into Full time- Part time status&#xd;&#xa;-- for data mining purposes&#xd;&#xa;--&#xd;&#xa;--&#xd;&#xa;----------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;Update PC &#xd;&#xa;       Set FTPT &#x3d; CASE&#xd;&#xa;			WHEN FTPT &#x3d; &#x27;F&#x27; THEN &#x27;1&#x27;&#xd;&#xa;			WHEN FTPT &#x3d; &#x27;P&#x27; THEN &#x27;2&#x27;&#xd;&#xa;			ELSE NULL&#xd;&#xa;		END&#x3b;&#xd;&#xa;&#xa;&#xa;----------------------------------------------------------------------&#xd;&#xa;-- Recoding Gender information into Class year&#xd;&#xa;-- for data mining purposes&#xd;&#xa;-- Varies for different intitution type&#xd;&#xa;--&#xd;&#xa;----------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;Update PC &#xd;&#xa;       Set CLASS &#x3d; CASE&#xd;&#xa;			WHEN CLASS &#x3d; &#x27;FR&#x27; THEN &#x27;1&#x27;&#xd;&#xa;			WHEN CLASS &#x3d; &#x27;SO&#x27; THEN &#x27;2&#x27;&#xd;&#xa;			WHEN CLASS &#x3d; &#x27;JR&#x27; THEN &#x27;3&#x27;&#xd;&#xa;			WHEN CLASS &#x3d; &#x27;SR&#x27; THEN &#x27;4&#x27;&#xd;&#xa;			WHEN CLASS &#x3d; &#x27;GR&#x27; THEN &#x27;5&#x27;&#xd;&#xa;			ELSE NULL&#xd;&#xa;		END&#x3b;&#xd;&#xa;&#xa;&#xd;&#xa;----------------------------------------------------------------------&#xd;&#xa;-- Recoding Letter Grade information to Numeric GPA &#xd;&#xa;-- for data mining purposes&#xd;&#xa;--&#xd;&#xa;-- These adhere to Marist grading policy. Might change if deployed elsewhere&#xd;&#xa;----------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;Update PC &#xd;&#xa;       Set LETTER_GRADE &#x3d; CASE&#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;A&#x27;  THEN &#x27;4.0&#x27;&#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;A-&#x27; THEN &#x27;3.7&#x27;&#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;B&#x2b;&#x27; THEN &#x27;3.3&#x27;&#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;B&#x27;  THEN &#x27;3.0&#x27;&#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;B-&#x27; THEN &#x27;2.7&#x27;&#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;C&#x2b;&#x27; THEN &#x27;2.3&#x27;&#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;C&#x27;  THEN &#x27;2.0&#x27;&#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;C-&#x27; THEN &#x27;1.7&#x27;&#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;D&#x2b;&#x27; THEN &#x27;1.3&#x27; &#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;D&#x27;  THEN &#x27;1.0&#x27;&#xd;&#xa;			WHEN LETTER_GRADE &#x3d; &#x27;F&#x27;  THEN &#x27;0.0&#x27;&#xd;&#xa;			ELSE NULL&#xd;&#xa;		END&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;       &#xa;       &#xa; </sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>OAAIDBDemo_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>586</xloc>
      <yloc>369</yloc>
      </entry>
