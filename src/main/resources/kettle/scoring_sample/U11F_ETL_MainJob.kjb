      <description/>
      <type>SQL</type>
      <sql>------------------------------------------------------------------------------------&#xd;&#xa;-- Add Indexes to speed up querying on merged tables.&#xd;&#xa;-- Crunching Sakai Event Log table for a semester to derive metric in similar lines&#xd;&#xa;-- to Gradebook. &#xd;&#xa;------------------------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- ALTER TABLE &#x60;pc&#x60; &#xa;-- ADD INDEX &#x60;pc_alternative_id_idx&#x60; &#x28;&#x60;ALTERNATIVE_ID&#x60; ASC, &#x60;RC_CLASS&#x60; ASC&#x29;&#x3b;&#xa;&#xa;--ALTER TABLE &#x60;sakaievents11f_complete&#x60; &#xa;--ADD INDEX &#x60;agga&#x60; &#x28;&#x60;ALTERNATIVE_ID&#x60; ASC, &#x60;COURSE&#x60; ASC, &#x60;EVENT&#x60; ASC&#x29;&#x3b;&#xa;&#xa;&#xa;drop table if exists mvCourseStudentEventQty&#x3b; &#xa;create table mvCourseStudentEventQty&#xa;SELECT S.COURSE, RC_CLASS, P.ALTERNATIVE_ID, EVENT, COUNT&#x28;&#x2a;&#x29; AS QTY&#xa;FROM  &#x24;&#x7b;SAKAIEVENTS&#x7d; S, PC P&#xa;WHERE S.ALTERNATIVE_ID &#x3d; P.ALTERNATIVE_ID&#xa;GROUP BY S.COURSE,RC_CLASS, P.ALTERNATIVE_ID,EVENT&#xa;ORDER BY S.COURSE,RC_CLASS, P.ALTERNATIVE_ID,EVENT&#x3b;&#xa;</sql>
