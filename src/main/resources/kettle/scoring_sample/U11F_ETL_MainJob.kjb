    <entry>
      <name>Agg_Scor</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;-------------------------------------------------------------------&#xd;&#xa;-- Calculating the actual score towards final grading by multiplying&#xd;&#xa;-- score and weight associated with it. Sum of all these scores for a&#xd;&#xa;-- student gives the final score against the grading rubric for the&#xd;&#xa;-- course.&#xd;&#xa;--&#xd;&#xa;-------------------------------------------------------------------&#xd;&#xa;&#xd;&#xa;drop table if exists Aux1_AggScores &#x3b;&#xd;&#xa;&#xd;&#xa;&#xa;create table Aux1_AggScores &#xa;select COURSE,alternative_id,sum&#x28;WEIGHT&#x29; TOTAL_WEIGHT&#xa;	  ,sum&#x28;SCORE &#x2a; WEIGHT&#x29;  as WEIGHTED_SCORE  &#xa;from GRADES &#xa; GROUP BY COURSE,ALTERNATIVE_ID&#xa;	  ORDER BY COURSE,ALTERNATIVE_ID&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>OAAIDBDemo_connect</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>505</xloc>
      <yloc>273</yloc>
      </entry>
    <entry>
      <name>AggScor2</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;-------------------------------------------------------------------------------&#xd;&#xa;-- Calculates a effective weighted score by taking the ratio of student score &#xd;&#xa;-- against the Total weight.&#xd;&#xa;--&#xd;&#xa;-------------------------------------------------------------------------------&#xd;&#xa;drop table if exists Aux2_AggScores&#x3b;&#xd;&#xa;&#xa;create table Aux2_AggScores&#xa;SELECT COURSE&#xa;      ,ALTERNATIVE_ID&#xa;      ,TOTAL_WEIGHT&#xa;      ,WEIGHTED_SCORE &#xa;      ,CASE&#xa;         WHEN TOTAL_WEIGHT&#x3c;&#x3e;0 then WEIGHTED_SCORE &#x2f;TOTAL_WEIGHT &#xa;         else  0&#xa;       end  as EFFWT_SCORE&#xa;FROM  Aux1_AggScores&#x3b;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>OAAIDBDemo_connect</connection>
